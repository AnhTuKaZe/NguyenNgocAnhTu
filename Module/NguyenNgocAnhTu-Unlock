[URL Rewrite]
# ==========================================
# üé¨ YOUTUBE AD BLOCKING - COMPLETE ‚úÖ
# ==========================================

# Block video ads in playback
^https:\/\/[\w-]+\.googlevideo\.com\/(?!(dclk_video_ads|videoplayback)).+&oad - reject-200

# Block YouTube ad stats & tracking
^https:\/\/(www|s)\.youtube\.com\/api\/stats\/ads - reject-200
^https:\/\/(www|s)\.youtube\.com\/(pagead|ptracking) - reject-200
^https:\/\/s\.youtube\.com\/api\/stats\/qoe\?adcontext - reject-200

# Remove ad tier from video URLs (improves quality)
(^https:\/\/[\w-]+\.googlevideo\.com\/(?!dclk_video_ads).+?)&ctier=L(&.+?),ctier,(.+) $1$2$3 302

# Block initplayback ads
^https:\/\/[\w-]+\.googlevideo\.com\/initplayback.+&oad - reject-img

[Map Local]
# ==========================================
# üé¨ YOUTUBE AD BLOCKING - MAP LOCAL ‚úÖ
# ==========================================

# Block initplayback ads
^https:\/\/[\w-]+\.googlevideo\.com\/initplayback.+&oad data-type=text data="" status-code=200

[Header Rewrite]
# Spotify
^https:\/\/(spclient\.wg\.spotify\.com|.+-spclient\.spotify\.com)\/user-customization-service\/v1\/customize$ header-del if-none-match

# Locket/RevenueCat - Better caching
^https:\/\/api\.revenuecat\.com header-add Cache-Control "public, max-age=31536000, immutable"
^https:\/\/api\.revenuecat\.com header-del ETag
^https:\/\/api\.revenuecat\.com header-del Last-Modified

[MITM]
hostname = *.googlevideo.com, -redirector*.googlevideo.com, www.youtube.com, s.youtube.com, youtubei.googleapis.com, api.revenuecat.com, spclient.wg.spotify.com, *-spclient.spotify.com, api-mobile.soundcloud.com, premium-*.truecaller.com, grpc.biliapi.net, app.bilibili.com, api.bilibili.com, live.bilibili.com, us-central1-alight-creative.cloudfunctions.net, api.picsart.com, api-account.kinemasters.com, api.sandbox.love, api.vivacut.com, api.funimate.com, lapi.photomath.net, lcs-mobile-cops.adobe.io, photos.adobe.io, app.algoriddim.com, buy.itunes.apple.com, web.vizmato.com, api.reface.video, api.busuu.com, api.calm.com, api.unfold.app, api-sub.meitu.com, api.camera360.com, user.camera360.com, *.xiuxiu.meitu.com, *.grammarly.com, notability.com, focos.oracle.bendingspoonsapps.com, restore-access.indream.app, *.agenda.com

