################################
#        [ Url Rewrite ]       #
################################

[Url Rewrite]

# ======= YouTube Ad Blocking (FULL PROTECTION!) ======= #
# Block video ads with oad parameter
^https?:\/\/[\w-]+\.googlevideo\.com\/(?!(dclk_video_ads|videoplayback\?)).+&oad - reject-img

# Block all ad requests
^https?:\/\/[\w-]+\.googlevideo\.com\/ptracking\? - reject-img
^https?:\/\/(www|s)\.youtube\.com\/api\/stats\/ads - reject-200
^https?:\/\/(www|s)\.youtube\.com\/(pagead|ptracking) - reject-200
^https?:\/\/s\.youtube\.com\/api\/stats\/qoe\?adcontext - reject-200

# Block YouTube mobile ads
^https?:\/\/[\w-]+\.youtube\.com\/get_midroll_ - reject-img
^https?:\/\/[\w-]+\.youtube\.com\/api\/stats\/ads - reject-img

# Remove ctier parameter for better quality
(^https?:\/\/[\w-]+\.googlevideo\.com\/(?!dclk_video_ads).+?)&ctier=L(&.+?),ctier,(.+) $1$2$3 302

# ======= Google Redirect Fix ======= #
^http:\/\/(www\.)?g\.cn https://www.google.com 302
^http:\/\/(www\.)?google\.cn https://www.google.com 302

################################
#        [ Map Local ]         #
################################

[Map Local]

# ======= YouTube Ad Initialization Blocking ======= #
^https?:\/\/[\w-]+\.googlevideo\.com\/initplayback.+&oad data-type=text data="" status-code=200

################################
#         [ Header ]           #
################################

[Header Rewrite]

# ======= YouTube Header Fix (Báº®T BUá»˜C!) ======= #
# Remove ad-related headers
^https:\/\/[\w-]+\.googlevideo\.com\/videoplayback header-del x-playback-session-id
^https:\/\/[\w-]+\.googlevideo\.com\/videoplayback header-del x-client-data

# ======= RevenueCat Headers ======= #
# For Locket, VSCO, and other RevenueCat apps
^https?://api.revenuecat.com/.+/(receipts$|subscribers/?(.*?)*$) header-del x-revenuecat-etag
^https?://api.revenuecat.com/.+/(receipts$|subscribers/?(.*?)*$) header-del X-RevenueCat-ETag

