[MITM]
# YouTube & Spotify
hostname = %APPEND% -redirector*.googlevideo.com, *.googlevideo.com, www.youtube.com, s.youtube.com, youtubei.googleapis.com, spclient.wg.spotify.com, *-spclient.spotify.com,
# RevenueCat (Locket, Duolingo, Calm)
hostname = %APPEND% api.revenuecat.com,
# TikTok
hostname = %APPEND% *.tiktokv.com, *.tiktokcdn.com, *.musical.ly,
# Social Media
hostname = %APPEND% i.instagram.com, www.instagram.com, api.twitter.com, twitter.com, x.com,
# Vietnam Apps
hostname = %APPEND% api.zingnews.vn, zingnews.vn, vnexpress.vn, api.vnexpress.vn, mp3.zing.vn, zingmp3.vn, api.zing.vn
enable = true
