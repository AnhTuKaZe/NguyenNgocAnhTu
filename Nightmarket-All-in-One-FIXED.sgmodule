[URL Rewrite]
# ==========================================
# ðŸŽ¬ YOUTUBE AD BLOCKING - ULTIMATE âœ…
# ==========================================

# Remove ad tier (CRITICAL for no ads + quality!)
(^https:\/\/[\w-]+\.googlevideo\.com\/(?!dclk_video_ads).+?)&ctier=L(&.+?),ctier,(.+) $1$2$3 302

# Block video ads (multiple patterns)
^https:\/\/[\w-]+\.googlevideo\.com\/(?!(dclk_video_ads|videoplayback)).+&oad - reject-200
^https:\/\/[\w-]+\.googlevideo\.com\/initplayback.+&oad - reject-img

# Block YouTube ad API
^https:\/\/(www|s)\.youtube\.com\/api\/stats\/ads - reject-200
^https:\/\/(www|s)\.youtube\.com\/(pagead|ptracking) - reject-200
^https:\/\/s\.youtube\.com\/api\/stats\/qoe\?adcontext - reject-200
^https:\/\/(www|s)\.youtube\.com\/api\/stats\/atr - reject-200

# Block additional ad requests
^https:\/\/(www|s)\.youtube\.com\/ptracking - reject-200
^https:\/\/(www|s)\.youtube\.com\/get_midroll - reject-200

